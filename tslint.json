{
    "defaultSeverity": "error",
    "extends": [
        "tslint-react",
        "tslint-config-prettier"
    ],
    "jsRules": {
    },
    "rules": {
        "member-access": false,
        "interface-name": [true, "never-prefix"],
        "ordered-imports": false,
        "quotemark": false,
        "no-console": false,
        "semicolon": false,
        "jsx-no-lambda": false
    },
    "linterOptions": {
        "exclude": [
            "config/**/*.js",
            "node_modules/**/*.ts"
        ]
    }
}